{
  "name": "image-to-vox",
  "version": "1.0.0",
  "description": "Converts an image to vox file",
  "engines": {
    "node": ">=12.18.0",
    "npm": ">=6.14.4"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vox",
    "voxel",
    "magicavoxel",
    "image"
  ],
  "author": "Roy Evan Sia",
  "license": "ISC",
  "homepage": "https://github.com/darklight721/image-to-vox#readme",
  "bugs": "https://github.com/darklight721/image-to-vox/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/darklight721/image-to-vox.git"
  },
  "files": [
    "lib/**/*"
  ],
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.0",
    "prettier": "2.4.1",
    "typescript": "^4.4.3"
  }
}
